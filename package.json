{
  "name": "@taskany/icons",
  "version": "1.0.0",
  "description": "Taskany Icons lib",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf ./icons ./es index.d.ts index.js index.js.map",
    "cleanup": "rm -rf ./src/icons/*.ts ./src/icons/*.tsx",
    "build": "npm run clean && npm run generate:icons && npm run build:cjm && npm run build:esm && npm run build:types && npm run cleanup",
    "build:cjm": "BABEL_ENV=cjs babel ./src/icons --out-dir ./dist --extensions .ts,.tsx",
    "build:esm": "BABEL_ENV=esm babel ./src/icons --out-dir ./dist/es --extensions .ts,.tsx",
    "build:types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .js,.ts,.tsx ./src",
    "prepare": "husky install",
    "type-check": "npx tsc --noEmit",
    "storybook": "BABEL_ENV=dev storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:icons": "npx ts-node ./src/generate/icons/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taskany-inc/icons.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@taskany/bricks": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@babel/cli": "7.22.5",
    "@babel/core": "7.22.5",
    "@babel/plugin-transform-modules-commonjs": "7.22.5",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@storybook/addon-essentials": "7.0.9",
    "@storybook/addon-interactions": "7.0.18",
    "@storybook/addon-links": "7.0.22",
    "@storybook/blocks": "7.0.21",
    "@storybook/react": "7.0.22",
    "@storybook/react-webpack5": "7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@taskany/bricks": "1.22.0",
    "@types/md5": "2.3.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.8",
    "babel-plugin-inline-react-svg": "2.0.2",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.12",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.7",
    "styled-components": "5.3.11",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "teenyicons": "0.4.1"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/taskany-inc/icons/issues"
  },
  "homepage": "https://github.com/taskany-inc/icons#readme"
}
